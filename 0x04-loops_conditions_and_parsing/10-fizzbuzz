#!/usr/bin/env bash
# Dispalys numbers 1 to 100
# display fizzbuzz if number is a multiple of 3 and 5
# display when number is a multiple of 3 and buzz for multiple of 5 else number

for i in {1..100}
do
	if (( i % 3 == 0 && i % 5 == 0 ))
	then
		echo "FizzBuzz"
	elif (( i % 3 == 0 ))
	then
		echo "Fizz"
	elif (( i % 5 == 0 ))
	then
		echo "Buzz"
	else
		echo "$i"
	fi
done

